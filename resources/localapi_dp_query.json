{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "type" : "object",
  "properties" : {
    "devId" : {
      "type" : "string"
    },
    "dps" : {
      "type" : "object",
      "patternProperties" : {
        "^[A-Za-z0-9]$": {
          "oneOf" : [
            {
              "type" : "string"
            },
            {
              "type" : "number"
            },
            {
              "type" : "boolean"
            }
          ]
        }
      }
    }
  },
  "required" : [
    "devId",
    "dps"
  ]
}
